CC=gcc

all : clean common.o server.o client.o main

common.o:
	$(CC) -c common/common.c

#Server
group.o: common.o
	$(CC) -c server/group.c

server.o: common.o group.o
	$(CC) -c server/server.c 

#client
client.o: common.o
	$(CC) -c client/client.c

main: server.o client.o
	$(CC) -o a.out main.c common.o group.o server.o client.o
	rm -f *.o

clean:
	rm -f *.o a.out

